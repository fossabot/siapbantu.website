<%- if controller_name != 'sessions' %>
  <%= link_to "Masuk", new_session_path(resource_name) %>&nbsp;&nbsp;&nbsp;&nbsp;
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Daftar", new_registration_path(resource_name) %>&nbsp;&nbsp;&nbsp;&nbsp;
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Lupa kata sandi?", new_password_path(resource_name) %>&nbsp;&nbsp;&nbsp;&nbsp;
<% end %>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Tidak menerima email konfirmasi?", new_confirmation_path(resource_name) %>&nbsp;&nbsp;&nbsp;&nbsp;
<% end %>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Tidak menerima email untuk membuka kunci?", new_unlock_path(resource_name) %>&nbsp;&nbsp;&nbsp;&nbsp;
<% end %>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Masuk dengan #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider) %><br />
  <% end %>
<% end %>
